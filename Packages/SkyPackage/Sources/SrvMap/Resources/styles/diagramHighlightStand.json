{
  "layers": [
    {
      "id": "DH_POINT_BACKGROUND",
      "type": "symbol",
      "iosFilter": "tp == 'stand'",
      "minzoom": 1,
      "source-layer": "diagramHighlightStand",
      "layout": {
        "icon-ignore-placement":true,
        "icon-allow-overlap": true,
        "icon-size": 0.65,
        "icon-image": "highlighted"
      }
    },
      {
      "id": "DH_LABEL",
      "type": "symbol",
      "iosFilter": "tp == 'stand'",
      "source-layer": "diagramHighlightStand",
      "minzoom": 1,
      "layout": {
        "text-allow-overlap": true,
        "text-font": ["Open Sans Bold"],
        "text-size": 17,
        "text-field": "{name}"
      },
      "paint": { "text-color": "tpt_wo_dim", "text-halo-color": "airfield_halo", "text-halo-width": 1.2 }
    }
  ]
}
